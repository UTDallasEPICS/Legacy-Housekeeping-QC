  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/components/scheduleComponents/ScheduleRoomPanel/scheduleRoomPanel.tsx"    // "src/components/authsComponents/signUpCardInVPSize"
  ],
  "exclude": ["node_modules"]
}
